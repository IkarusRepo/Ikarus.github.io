concept <ref refid="dc/d42/a01591" kindref="compound">Ikarus::Concepts::LinearSolverCheck</ref> =  requires(LinearSolver&amp; linearSolver, MatrixType&amp; Ax, VectorType&amp; vec) {
      linearSolver.analyzePattern(Ax);
      linearSolver.factorize(Ax);
      linearSolver.solve(vec, vec);
    }    </initializer>
